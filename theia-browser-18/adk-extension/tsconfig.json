{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2017",
    "lib": ["ES2017", "DOM"],
    "types": [],
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "jsx": "react",
    "strict": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "outDir": "lib"
  },
  "include": ["src"]
}
